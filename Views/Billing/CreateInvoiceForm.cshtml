@model BabzRent.ViewModels.BillingViewModels.CreateInvoiceFormViewModel
@{
    ViewBag.Title = "CreateInvoiceForm";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script>
    function goBack() {
        window.history.back();
    }
</script>

<br>
<p class="btn btn-primary" onclick="goBack()"><< Back</p>

<h2>CreateInvoiceForm</h2>

<h2>Location</h2>
@using (Html.BeginForm("CreateInvoice", "Billing"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.tenancyId)
        @Html.DropDownListFor(m => m.tenancyId, new SelectList(Model.Tenancies, "Id", "Name"), "Select Location", new { @class = "form-control" })
        <p>This list only contains active tenancies and those that expired within the last 3 months.</p>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
}
